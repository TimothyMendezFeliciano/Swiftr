    "firebase": "^10.5.2",
    "firebase-admin": "^11.11.0",
    "framer-motion": "^10.16.4",
    "next": "13.5.6",
    "react": "^18",
    "react-dom": "^18",
    "react-qr-code": "^2.0.12",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "eslint": "^8",
    "eslint-config-next": "13.5.6",
