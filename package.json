{
  "name": "swiftr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "firebasedocker": "docker run --rm -p=9000:9000 -p=8080:8080 -p=4000:4000 -p=9099:9099  -p=8085:8085 -p=5001:5001  -p=9199:9199 --env GCP_PROJECT=<swiftpr> --env ENABLE_UI=true spine3/firebase-emulator",
    "fire": "firebase emulators:start",
    "seed": "ts-node ./seeds/seed.ts && tsc --watch",
    "serve:seeded": "npm run build && firebase emulators:exec --inspect-functions --ui --only functions,firestore,auth,pubsub,storage \"npm run seed\""
  },
  "dependencies": {
    "@firebase/rules-unit-testing": "^3.0.1",
    "@google-cloud/firestore": "^7.1.0",
    "@headlessui/react": "^1.7.17",
